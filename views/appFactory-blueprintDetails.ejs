<style>
.bluePrintDetailsContainer {

}
</style>
<div class="bluePrintDetailsContainer">


<form class="form-horizontal" role="form">
     <div class="form-group">
      <label for="stackNameInput" class="col-sm-2 control-label">OS</label>
  		<div class="col-sm-10">
        <span type="text" class="form-control" ><%=blueprint.os%></span>
       </div>  
     </div>

     <div class="form-group">
      <label for="stackNameInput" class="col-sm-2 control-label">Instance Type</label>
  		<div class="col-sm-10">
        <span type="text" class="form-control" ><%=blueprint.instanceType%></span>
       </div>  
     </div>

     <div class="form-group">
      <label for="stackNameInput" class="col-sm-2 control-label">Number Of Instances</label>
  		<div class="col-sm-10">
        <span type="text" class="form-control" ><%=blueprint.numberOfInstance%></span>
       </div>  
     </div>
     
     <div class="form-group">
      <label for="stackNameInput" class="col-sm-2 control-label">Instance Validity</label>
  		<div class="col-sm-10">
        <span type="text" class="form-control" ><%=blueprint.expirationDays%></span>
       </div>  
     </div>


     <%
     if(blueprint.runlist && blueprint.runlist.length) {
     %>
     <div class="form-group">
      <label for="stackNameInput" class="col-sm-2 control-label">Runlist</label>
  		<div class="col-sm-10">
        <ul>
         <%
         for(var i=0;i<blueprint.runlist.length;i++) {
         %>
         <li style="list-style: none outside none"><%=blueprint.runlist[i]%></li>
         <%}%>
       </ul>
       </div>  
     </div>
     <%}%>


</form>


</div>